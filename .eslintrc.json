{
  "extends": [
    "airbnb-typescript/base", 
    "plugin:prettier/recommended"
    // "plugin:jest/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/prefer-default-export": "true",
    "no-unused-vars": "warn",
    "no-unused-labels": "warn",
    "sort-imports": "warn",
    "no-duplicate-imports": "warn"
  }
}
